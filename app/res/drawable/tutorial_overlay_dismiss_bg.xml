<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                    android:color="@color/blue_base"/>
            <stroke
                    android:width="1dip"
                    android:color="@color/blue_stroke" />
            <corners
                    android:radius="3dip"/>
        </shape>
    </item>
</selector>